name: dockercompose2520575775406632510
services:
  easy.hosts:
    build:
      context: C:\Users\User\Desktop\Easy.Hosts
      dockerfile: Easy.Hosts/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Easy.Hosts
      target: base
    container_name: easyhosts.api
    depends_on:
      sqlserver:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: easyhosts:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\User\Desktop\Easy.Hosts\Easy.Hosts
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\Desktop\Easy.Hosts
      target: /src
      bind:
        create_host_path: true
  sqlserver:
    container_name: easyhostsdb
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: e@syhosts!
    expose:
    - "1433"
    hostname: sqlserver
    image: mcr.microsoft.com/mssql/server:2017-latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "5434"
      protocol: tcp
networks:
  default:
    name: dockercompose2520575775406632510_default